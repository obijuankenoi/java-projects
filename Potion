package Gamemode;
public class Potion extends Equipment{
	static String mod,name;
	static int val,amount,id;
	static Equipment potion;
	public Potion(String newMod, String newName, String newSecond, String newPotions, int newValue, int newDmg,int newId) {
		super(null, null, newPotions, newValue, newDmg, newId);
		mod=newMod;
		name=newPotions;
		val=newValue;
		amount=newDmg;
		id=newId;
	}
	public static String getMod(){
		return mod;
	}
	public static void setMod(String newMod){
		mod=newMod;
	}
	public static String getPotionName(){
		return name;
	}
	public static void setPotionName(String newName){
		name=newName;
	}
	public static int getValue(){
		return val;
	}
	public static void setPotionValue(int newVal){
		val=newVal;
	}
	public static int getPotionAmount(){
		return amount;
	}
	public static void setPotionAmount(int newAmount){
		amount=newAmount;
	}
	public static Equipment getPotion(){
		return potion;
	}
	public static void setPotion(Equipment newPotion){
		potion=newPotion;
	}
	public static int getId(){
		return id;
	}
	public static void setId(int newId){
		id=newId;
	}
}
