package Gamemode;
import java.io.IOException;
public class gameMusic{
	public static void startMusic(){
		String command = "\"C:/Program Files (x86)/Windows Media Player/wmplayer.exe\" \"C:/RuneScape 2007 Soundtrack- Greatest Hits.mp3\"";
		try {
		    Process p = Runtime.getRuntime().exec(command);}
		catch (IOException e) {
		    e.printStackTrace();
		}
	}
}
