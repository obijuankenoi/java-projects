
public class Equipment {
	private int dmg,value;
	private String desc;
	private String[] mod,name,potion;
	public Equipment(String[] arr1,String[] arr2,String[] arr3,int value,int dmg){
		this.mod=arr1;
		this.name=arr2;
		this.potion=arr3;
		this.value=value;
		this.dmg=dmg;
	}
	public Equipment(String name2, String desc2, int value2, int dmg2) {
	}
	public String[] getName(){
		return name;
	}
	public String[] getMod(){
		return mod;
	}
	public String getDesc(){
		return desc;
	}
	public int getDmg(){
		return dmg;
	}
	public int getValue(){
		return value;
	}
}
