package Gamemode;

public class Equipment {
	private int dmg,value;
	private String desc;
	private String mod,name,potion;
	public Equipment(String mod,String name,String second,String potions, int value,int dmg){
		this.mod=mod;
		this.name=name;
		this.potion=potion;
		this.value=value;
		this.dmg=dmg;
	}
	public Equipment(String name2, String desc2, int value2, int dmg2) {
	}
	public String getName(){
		return name;
	}
	public String getMod(){
		return mod;
	}
	public String getDesc(){
		return desc;
	}
	public int getDmg(){
		return dmg;
	}
	public int getValue(){
		return value;
	}
}
