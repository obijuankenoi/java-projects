package Gamemode;

public class Equipment {
	private static int dmg,value;
	private static String mod,name,potion,desc;
	public Equipment(String newMod,String newName,String newSecond,String newPotions, int newValue,int newDmg){
		mod=newMod;
		name=newName;
		potion=newPotions;
	    value=newValue;
		dmg=newDmg;
	}
	public static String getName(){
		return name;
	}
	public static String getMod(){
		return mod;
	}
	public static String getDesc(){
		return desc;
	}
	public static String getPotionName(){
		return potion;
	}
	public static int getDmg(){
		return dmg;
	}
	public static int getValue(){
		return value;
	}
}
